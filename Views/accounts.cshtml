<head>
  <link rel="stylesheet" href="/Content/css/bootstrap.min.css" type="text/css">
  <link rel="stylesheet" href="/Content/css/styles.css" type="text/css">
</head>
<body>
  <h1>List of All Accounts</h1>
  @foreach (var account in Model)
  {
    <ul>
      <li><a href="/accounts/@account.GetId()/@account.GetFirstName()">@account.GetFirstName() @account.GetLastName()</a></li>
    </ul>
  }
  <a href="/"><h1>Back Home</h1></a>
</body>
