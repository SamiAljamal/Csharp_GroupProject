<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title></title>
  </head>
  <body>
    <h3>@Model.GetTitle()</h3>

    <ul>
      @foreach(var word in Model.UniqueWordCount())
      {
        <li>@word.Key : @word.Value</li>
      }
    </ul>
    <a href="/">Home</a>
  </body>
</html>
